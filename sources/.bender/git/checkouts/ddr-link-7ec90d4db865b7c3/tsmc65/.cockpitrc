[cockpit]
  # Do not change this section
  config = 2020

  # Still Work in Progress

[library]
  # Setup files will be generated for these libraries/rams.
  # 
  # - only libraries that can be found in the "kit.ascii" database
  #   can be used (see cockpit.log for the full unix path)
  # - use "-update" or "-rewrite" for the changes to take effect  
  #
  # The following libraries are available:
  #   technology    : tsmc65
  #
  #                   
  #   designkit     : 1p9m_6X1Z1U_ALRDL_MIM_2.0fF_1.2V-2.5V_RF__v1.7b
  #
  #   libraries     : Low VT (fast)        Regular VT           High VT (slow)
  #      (tsmc)       tcbn65lplvt_200a     tcbn65lp_200a        tcbn65lphvt_200a  ++ OBSOLETE ++
  #      (arm)        sc8_base_lvt_r0p0    sc8_base_rvt_r0p0    sc8_base_hvt_r0p0
  #                                        sc8_eco_rvt_r0p0
  #                                                             sc8_pmk_hvt_r0p0
  #                                        ======   sc8_pmk_rvt_hvt_r0p0   =====
  #                   ============       sc8_pmk_lvt_hvt_r0p0      =============
  #
  #   pad-libraries : tpdn65lpnv2_140b                                            ++ OBSOLETE ++
  #                   tpdn65lpnv2od3_200a   - 1.0,1.2V/1.8,2.5,3.3V, (over-drive to 3.3V), Regular, I/O Linear Universal Standard
  #
  #   pad-bond      : tpbn65v_200a
  #
  #   memories      : tsmc65mem                                                   ++ OBSOLETE ++
  #   mem compiler  : tsmc65mem_rf_2p_hde_hvt_rvt_r0p0
  #       (arm)       tsmc65mem_rf_sp_hdf_hvt_rvt_r0p0
  #                   tsmc65mem_sram_dp_hde_hvt_rvt_r0p0
  #                   tsmc65mem_sram_sp_hde_hvt_rvt_r0p0
  #                   tsmc65mem_sram_sp_hse_hvt_rvt_r1p0
  #   rom compiler  : tsmc65mem_rom_via_hdd_rvt_rvt_r0p0
  #
  #   ip blocks     : tsmc65_FLL_v0.1       Top Metal Layer  : M5
  #
  
 libs    = tsmc65 1p9m_6X1Z1U_ALRDL_MIM_2.0fF_1.2V-2.5V_RF__v1.7b \
            sc8_base_lvt_r0p0 sc8_base_rvt_r0p0 sc8_base_hvt_r0p0  \
            tpdn65lpnv2od3_200a tpbn65v_200a

  # see //usr/pack/tsmc-65-kgf/arm/tsmc/cln65lp/<ram_type>/<version>/gdsii.dz/ to see which macros have already been generated
  # enter the name WITHOUT '.gds' in the list below
  # if the macro not already generated, send a e-mail to <dz@ee.ethz.ch> to ask for new macros.

  macros  = 
  
[version]
  # Setup files will be generated for these software versions. 
  # - only versions that can be found in the "kit.ascii" database
  #   can be used (see cockpit.log for the full unix path)
  # - if you change a version you might need to adjust some
  #   startups in the [command] section too... 
  # - use "-update" or "-rewrite" for the changes to take effect  

  # WARNING: The primary purpose is to keep version settings within each
  # project. Fiddling around will most likely result in an unsupported
  # combinations and perl errors. Better update this file as a whole
  # with '-updaterc'. 

  #### modelsim  =  questa-2019.3  # not working because in the 'get_tool_values_regexp' the tool name will be get out of it.
  modelsim  =  modelsim-2019.3
  synopsys  =  synopsys-2019.03
  tetramax  =  tetramax-2019.03
  innovus   =  innovus-19
  dfii      =  ic-6
  calibre   =  calibre-2019.2

[command]
  # Startup commands available through the GUI buttons.
  # - startup commands can be modified but need to be compatible with the
  #   setup file versions selected in the [version] section 
  # - to add more modelsim/synopsys startups to the GUI just put your own entries
  #   of the form "(modelsim|synopsys):<label> = <command>" there 
  # - changes will take effect the next time you start

  emacs                        =  emacs -f vhdl-mode
  modelsim                     =  questa-2019.3 vsim
  synopsys:DC Shell            =  xterm -g 120x40 -T synopsys-2019.03 -e "bash -c 'TERM=xterm-color synopsys-2019.03 dc_shell -64bit | tee view_command.log'"
  synopsys:DesignVision        =  xterm -g 120x40 -T synopsys-2019.03 -e "bash -c 'TERM=xterm-color synopsys-2019.03 design_vision -64bit | tee view_command.log'"
  tetramax                     =  synopsys-2019.03 tmax
  innovus                      =  xterm -g 136x40 -T innovus-19.10 -e "cds_innovus-19.10.000 innovus"
  calibre                      =  xterm -g 120x40 -T calibredrv-2019.2 -e "./start_calibre 2019.2"
  dfii                         =  ./start_dfii
  dz:llama                     = llama

